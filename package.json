  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@playwright/test": "^1.54.2",
    "@storybook/react": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@testing-library/dom": "^10.4.1",
