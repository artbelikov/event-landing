{
  "name": "Event landing frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc && vite build",
    "build:test": "tsc && vite build --mode test",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "preview:test": "vite preview --mode test",
    "preview:prod": "vite preview --mode production",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --cache",
    "eslint:fix": "eslint . --cache --fix",
    "eslint:check": "eslint . --cache --max-warnings 0",
    "test": "vitest --mode test",
    "test:run": "vitest run --mode test",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "monitor": "node scripts/monitor.js",
    "generate:api-client": "cd ../event-landing-backend && bun run generate:api-client",
    "mcp:start": "npx @agentdeskai/browser-tools-server@1.2.0"
  },
